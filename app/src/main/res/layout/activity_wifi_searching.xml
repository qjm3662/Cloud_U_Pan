<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_wifi_searching"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.qjm3662.cloud_u_pan.Wifip2pDirect.WifiSearching">
    <com.guo.duoduo.library.RadarScanView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

    </com.guo.duoduo.library.RadarScanView>

    <com.guo.duoduo.randomtextview.RandomTextView
        android:id="@+id/random_textview"
        android:layout_width="300dp"
        android:layout_height="300dp"
        android:layout_centerInParent="true">

    </com.guo.duoduo.randomtextview.RandomTextView>

    <com.qjm3662.cloud_u_pan.Widget.EasyButton
        android:id="@+id/btn_sendFile"
        style="@style/EasyButtonStyle"
        android:layout_marginTop="20dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="25dp"
        android:text=" 发送文件 " />

</RelativeLayout>
